function chunkArrayInGroups(arr, size) {
var z=[];
  
 while(arr.length>0)
 {
      var arr3=[];
arr3=[arr.splice(0,size)];


z.push(...arr3);
console.log(z)
 }

  return z;
}

chunkArrayInGroups([0, 1, 2, 3, 4, 5], 2);

